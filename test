import turtle

t=turtle.Turtle()
t.penup()
t.goto(-30,50) #centering in the screen
t.pendown()
t.pensize(10)
t.pencolor("red")
t.forward(100)
t.goto(20,50)
t.right(90)
t.forward(100)

t=turtle.Turtle()
t.penup()
t.goto(-30,50) #centering in the screen
t.pendown()
t.pensize(10)
t.pencolor("red")
t.right(90)
t.forward(150)
t.goto(-30,50)
t.circle(50,None,100)

t=turtle.Turtle()
t.penup()
#draw straight line
t.goto(-30,50) #centering in the screen
t.pendown()
t.pensize(10)
t.pencolor("red")
t.right(180)
t.circle(50,180)

t=turtle.Turtle()
t.penup()
t.goto(-30,50) #centering in the screen
t.pendown()
t.pensize(10)
t.pencolor("red")
t.forward(100)
t.goto(20,50)
t.right(90)
t.forward(100)

t=turtle.Turtle()
t.penup()
t.setpos(-30,50) #centering in the screen
t.pendown()
t.pensize(10)
t.pencolor("red")
t.forward(100)
t.backward(100)
t.right(90)
t.forward(100)
t.left(90)
t.forward(100)
t.backward(100)
t.right(90)
t.forward(100)

t=turtle.Turtle()
t.penup()
t.goto(-30,50) #centering in the screen
t.pendown()
t.pensize(10)
t.pencolor("red")
t.right(65)
t.forward(100)
t.left(130)
t.forward(100)
 
t.penup()
t.goto(13,-43)
t.left(25)
t.pendown()
t.backward(100)

t=turtle.Turtle()
t.penup()
t.setpos(-30,50) #centering in the screen
t.pendown()
t.pensize(10)
t.pencolor("red")
t.forward(100)
t.backward(100)
t.right(90)
t.forward(100)
t.left(90)
t.forward(100)
t.backward(100)
t.right(90)
t.forward(100)
t.left(90)
t.forward(100)

t=turtle.Turtle()
t.penup()
t.goto(-30,50) #centering in the screen
t.pendown()
t.pensize(10)
t.pencolor("red")
t.right(90)
t.forward(150)
t.goto(-30,50)
t.right(-90)
t.circle(-50,180,100)
t.penup()
t.goto(0,-40)
t.left(135)
t.pendown()
t.forward(70)
